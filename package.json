{
  "name": "reactDemo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrent \"node ./bin/www\" \"watchify entry.js -v -t babelify -o public/javascripts/index.js\"",
    "compile": "browserify entry -t babelify -o public/javascripts/index.js"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "hbs": "~2.9.0",
    "morgan": "~1.5.3",
    "react": "^0.13.3",
    "serve-favicon": "~2.2.1"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "concurrently": "^0.1.1",
    "watchify": "^3.3.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
